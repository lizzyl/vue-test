
{
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2, { "VariableDeclarator": 2 }],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "no-cond-assign": ["error", "always"],
    "no-console": "warn"
  },
  "globals": {
    "$": true,
    "axios": true,
    "Vue": true
  },
  "env": {
    "browser": true
  }
}
